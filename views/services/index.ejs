<div class="row">
<% services.forEach((service) => { %>
  <div class="col s12 m6"><span class="flow-text">
    <a href="/services/<%= service.id %>">
      <img class="thumbnail" src="<%= service.image %>" alt="<%= service.name %>">
      <h2><%= service.name %></h2>
      <h3><%= service.category %></h3>
      <h3>Ratings:</h3>
      <ul>
        <% service.averageRatings.forEach(rating => { %>
          <p><%= rating.name %>: <%= rating.avg %></p>
        <% }); %>
      </ul>
    </a>
</span>
</div>
    </div>
<% }) %>
<% if(locals.isAuthenticated) { %>
  <a href="/services/new" class="button">Add Service</a>
  <% } %>

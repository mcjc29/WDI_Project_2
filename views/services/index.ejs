<div class="container">
  <div class="row">

    <% if(locals.isAuthenticated) { %>


    <div class="col-lg-4 col-md-6 col-sm-9 box">
    <a href="/services/new %>">
    <div class="logo-container" style="background-image: url('https://cdn.pixabay.com/photo/2016/10/10/01/49/plus-1727487_960_720.png'); height: 200px;"></div>
    <div class= "service-name"
    <h1>Add a new service</h1>
    </a>
    </div>
      </div>
      <% } %>

  <% services.forEach((service) => { %>
    <div class="col-lg-4 col-md-6 col-sm-9 box">
    <a href="/services/<%= service.id %>">
    <div class="logo-container" style="background-image: url('<%= service.image %>'); height: 200px;"></div>
    <div class= "service-name"
    <h1><%= service.name %></h1>
    </a>
    <h3><%= service.category %></h3>
    </div>
      </div>
      <% }) %>


    </div>
  </div>

<form method="POST" action="/services">
  <input type="hidden" name="_method" value="PUT">
  <div>
    <label for="name">Name</label>
    <input type="text" name="name" id="name" required value="<%= service.firstName %>">
  </div>
  <fieldset>
    <legend>Address</legend>
    <div>
      <label for="line1">Line 1</label>
      <input type="text" name="address[line1]" id="line1" required value="<%= service.line1 %>">
    </div>
    <div>
      <label for="line2">Line 2</label>
      <input type="text" name="address[line2]" id="line2" value="<%= service.line2 %>">
    </div>
    <div>
      <label for="city">City</label>
      <input type="text" name="address[city]" id="city" required value="<%= service.city %>">
    </div>
    <div>
      <label for="postcode">Postcode</label>
      <input type="text" name="address[postcode]" id="postcode" required value="<%= service.postcode %>">
    </div>

  </fieldset>
  <div>
    <label for="stars">Stars</label>
    <input type="number" name="stars" id="stars" min="1" max="5" required value="<%= service.stars %>">
  </div>
  <div>
    <label for="image">Image</label>
    <input type="text" name="image" id="image" required value="<%= service.image %>">
  </div>
  <div>
    <label for="website">Image</label>
    <input type="text" name="website" id="website" required value="<%= service.website %>">
  </div>
  <button class="button">Register</button>
</form>
